// lear more about this file in the docs: https://pris.ly/d/prism-schema
datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
    binaryTargets = ["native"]
}

model Todo {
    uid String @id @default(cuid())
    created_at DateTime
    text String
    done Boolean
}